{
    "name": "parallax/masked-field",
    "description": "A Laravel Nova field providing a masked value.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Parallax\\MaskedField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parallax\\MaskedField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
